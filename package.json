{
  "name": "pi-langlearn",
  "version": "0.1.0",
  "description": "Duolingo-esque language learning overlay for Pi",
  "type": "module",
  "files": [
    "dist",
    "src",
    "README.md",
    "AGENTS.md",
    "docs/specs"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "prepublishOnly": "bun run build",
    "release": "bun run scripts/release.ts",
    "typecheck": "tsc -p tsconfig.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "pi": {
    "extensions": [
      "./src/index.ts"
    ]
  },
  "keywords": [
    "pi-package",
    "pi",
    "pi-coding-agent",
    "extension",
    "tui",
    "language-learning",
    "srs"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.1",
    "typescript": "^5.6.3"
  }
}
